<!DOCTYPE html>
<html>
<head>
  <title>Cms Engine</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
		<% flash.each do |key, value| %>
			<div id ="notice">
				<%= content_tag(:div, value, :class => "flash #{key}") %>
			</div>
		<% end %>

	<%= yield %>

</body>
</html>
